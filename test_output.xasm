@pool: "INT SOURCE: $int PTR_ADDR: $ptr"

_vmstart:
    push 0;
    mov reg0,esp;
    sub reg0,8;
    push reg0;

_func_max:
    pop reg0;
    pop reg1;
    if_max reg0,ret1;
    jneq_a eip,1; // is different from jneq its format: <offset>,<add>
    push 0;
    push 1;
    ret;

_func_main_int_ptrchar:
    push [0];
    mov reg0,esp;
    sub reg0,24;
    push *reg0;
    mov reg0,esp;
    sub reg0,24;
    push reg0;
    call _@std_print_any;
    mov reg10 1145141919810; // return value register
    ret;